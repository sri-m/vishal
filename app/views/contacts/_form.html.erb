<%= semantic_form_for @contact do |f| %>
  <%= f.inputs do %>
    <%= f.input :name, :label => "Titles", :hint => "Can't blank", :required => true %>
    <%= f.input :gender, :as => :radio, :collection => ["Male", "Female"] %>
    <%= f.input :category, as: :select, :collection => Registration.pluck(:name) %>
    <%= f.input :address %>
  <% end %>

  <%= f.actions do %>
    <%= f.action :submit, :as => :input %>
  <% end %>
<% end %>
